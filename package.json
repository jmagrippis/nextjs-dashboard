		"@types/node": "^20.10.0",
		"@vercel/analytics": "^1.1.1",
		"autoprefixer": "^10.4.16",
		"clsx": "^2.0.0",
		"negotiator": "^0.6.3",
		"next": "^14.0.3",
		"next-auth": "beta",
		"postcss": "^8.4.31",
		"react": "^18.2.0",
		"react-dom": "18.2.0",
		"tailwindcss": "^3.3.5",
		"typescript": "^5.3.2",
		"use-debounce": "^10.0.0",
